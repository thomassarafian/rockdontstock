/config/application.yml

# Ignore environment files
.env
.env.local
.env.development
.env.production
.env.test

# Ignore deployment files with sensitive data
DEPLOYMENT_GUIDE.md
RAILWAY_DEPLOYMENT.md
railway.json

# Security documentation (keep private)
